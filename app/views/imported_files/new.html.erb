<div class="container">
<div class="row">
<div class="col-md-6 col-md-offset-3">
<h1>Import products</h1>
<h3>by CSV file</h3>
</div>
</div>
<div class="row">
<div class="col-md-6 col-md-offset-3">
<%= form_for(@imported_file) do |f|%>
  <% if @imported_file.errors.any? %>
    <div id="error_explanation">
      <ul>
      <% @imported_file.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
    <%= f.label :data %>
    <%= f.file_field :data, required: true, class: 'form-control' %>
  </div>
  <%= f.submit 'Import file', class: 'btn btn-primary'%>
<% end %>
</div>
